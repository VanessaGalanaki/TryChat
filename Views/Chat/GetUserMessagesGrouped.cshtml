@model IEnumerable<string>

@{
    ViewData["Title"] = "GetUserMessagesGrouped";
}

<h2>GetUserMessagesGrouped</h2>

<table class="table">
    <thead>
        <tr>

            <th>
                @Html.DisplayNameFor(model => model)
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                <td>
                    @Html.DisplayFor(modelItem => item)
                </td>


            </tr>
        }
    </tbody>
</table>
